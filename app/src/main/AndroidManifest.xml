<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nkucs1416.simpbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".record.RecordActivity" />
        <activity android:name=".account.AccountActivity" />
        <activity android:name=".statement.StatementActivity" />
        <activity android:name=".setting.SettingActivity" />
        <activity android:name=".edit.EditActivity" />
    </application>

</manifest>